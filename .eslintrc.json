{
    "extends": "airbnb",
    "installedESLint": true,
    "ecmaFeatures": {
        "jsx": true
    },
    "plugins": [
        "react",
        "import"
    ],
    "rules": {
        "react/jsx-filename-extension": [0],
        "no-use-before-define": [0],
        "react/prop-types": [0]
    }
}
